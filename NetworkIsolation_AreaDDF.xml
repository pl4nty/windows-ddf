        </MSFT:AllowedValues>
        <MSFT:GpMapping GpEnglishName="WF_NetIsolation_EnterpriseCloudResources" GpAreaPath="NetworkIsolation~AT~Network~WF_Isolation" GpElement="WF_NetIsolation_EnterpriseCloudResourcesBox" />
