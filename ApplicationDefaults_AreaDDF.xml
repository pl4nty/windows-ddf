        </MSFT:Applicability>
        <MSFT:GpMapping GpEnglishName="DefaultAssociationsConfiguration" GpAreaPath="WindowsExplorer~AT~WindowsComponents~WindowsExplorer" GpElement="DefaultAssociationsConfiguration_TextBox" />
        <MSFT:ConflictResolution>LastWrite</MSFT:ConflictResolution>
      </DFProperties>
